+1)    \n","        rec_movie_indices = sorted(list(zip(indices.squeeze().tolist(),distances.squeeze().tolist())),key=lambda x: x[1])[:0:-1]\n","        recommend_frame = []\n","        for val in rec_movie_indices:\n","            movie_idx = final_dataset.iloc[val[0]]['movieId']\n","            idx = movies[movies['movieId'] == movie_idx].index\n","            recommend_frame.append({'Title':movies.iloc[idx]['title'].values[0],'Distance':val[1]})\n","        df = pd.DataFrame(recommend_frame,index=range(1,n_movies_to_reccomend+1))\n","        return df\n","    else:\n","        return \"Movie not found.\""],"execution_count":16,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":343},"id":"mcS-wrL_ApLA","executionInfo":{"status":"ok","timestamp":1621164918531,"user_tz":-330,"elapsed":879,"user":{"displayName":"Vaibhav Singh","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14Ghoe02G6iI0wa3PHOUWsbdA44XRMkvOvKXoa5WTTg=s64","userId":"03786318363282038411"}},"outputId":"13647f4b-477c-45ef-860d-8f4b193bba6e"},"source":["get_movie_recommendation('Avatar')"],"execution_count":37,"outputs":[{"output_type":"execute_result","data":{"text/html":["<div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>Title</th>\n","      <th>Distance</th>\n","    </tr>\n","  </thead>\n","  <tbody>\n","    <tr>\n","      <th>1</th>\n","      <td>Zombieland (2009)</td>\n","      <td>0.398180</td>\n","    </tr>\n","    <tr>\n","      <th>2</th>\n","      <td>Inception (2010)</td>\n","      <td>0.393521</td>\n","    </tr>\n","    <tr>\n","      <th>3</th>\n","      <td>I Am Legend (20